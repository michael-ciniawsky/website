    "gulp-postcss": "^6.1.0",
    "gulp-posthtml": "^1.5.2",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "html-webpack-plugin": "^2.14.0",
    "nyc": "^6.1.1",
    "postcss-bem": "^0.4.1",
    "postcss-loader": "^0.8.0",
    "posthtml-bem": "^0.2.2",
