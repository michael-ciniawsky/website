{
  "name": "michael-ciniawsky.de",
  "version": "0.0.1",
  "description": "Website",
  "main": "server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "echo '=> Linting' standard && nyc ava './test/**/*.test.js' --require ./test/ava-setup.js",
    "cover": "echo '=> Coverage' && nyc --reporter=lcov --reporter=text npm run test",
    "builds": "echo '=> Building' && gulp",
    "bundle": "echo '=> Bundling' && NODE_ENV=Production webpack --config webpack.config.js",
    "serve": "echo '=> Starting DEV' && NODE_ENV=Development webpack-dev-server --config webpack.config.server.js",
    "start": "echo '=> Starting Server' && nodemon --exec node server"
  },
  "ava": {},
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "dependencies": {
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-posthtml": "^1.0.3",
    "express-session": "^1.12.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
